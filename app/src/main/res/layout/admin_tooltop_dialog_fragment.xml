<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="700dp"
    android:layout_height="200dp"
    android:layout_centerInParent="true"
    android:background="@color/shadowGray">

    <Button
        android:id="@+id/admin_tooltip_dialog_fragment_button_lock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="Lock Screen"
        app:layout_constraintBottom_toBottomOf="@+id/admin_tooltip_dialog_fragment_button_print"
        app:layout_constraintEnd_toStartOf="@+id/admin_tooltip_dialog_fragment_button_print"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/admin_tooltip_dialog_fragment_button_print" />

    <Button
        android:id="@+id/admin_tooltip_dialog_fragment_button_print"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:text="Test Print"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/admin_tooltip_dialog_fragment_button_settings"
        app:layout_constraintStart_toEndOf="@+id/admin_tooltip_dialog_fragment_button_lock"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/admin_tooltip_dialog_fragment_button_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:text="Admin Settings"
        app:layout_constraintBottom_toBottomOf="@+id/admin_tooltip_dialog_fragment_button_print"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/admin_tooltip_dialog_fragment_button_print"
        app:layout_constraintTop_toTopOf="@+id/admin_tooltip_dialog_fragment_button_print" />

    <Button
        android:id="@+id/button7"
        android:tag="@string/dialog_fragment_button_close_tag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:text="Close"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>